{
  "name": "Hakim Dotnet",
  "image": "ghcr.io/shekohex/hakim-base:latest",
  "build": {
    "args": {
      "GITHUB_TOKEN": "${localEnv:GITHUB_TOKEN}"
    }
  },
  "features": {
    "../../../features/src/dotnet": {
      "version": "9.0",
      "additionalVersions": "10.0-preview"
    },
    "../../../features/src/nodejs": {
      "version": "24.13.0"
    },
    "../../../features/src/bun": {
      "version": "1.3.8"
    },
    "../../../features/src/uv": {},
    "../../../features/src/openchamber": {},
    "../../../features/src/openclaw": {},
    "../../../features/src/lazyvim": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-dotnettools.csdevkit"
      ]
    }
  }
}
