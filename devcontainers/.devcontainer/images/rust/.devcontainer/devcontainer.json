{
  "name": "Hakim Rust",
  "image": "ghcr.io/shekohex/hakim-tooling:latest",
  "build": {
    "args": {
      "GITHUB_TOKEN": "${localEnv:GITHUB_TOKEN}"
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/rust:1": {
      "version": "1.93.0",
      "profile": "minimal"
    },
    "../../../features/src/lazyvim": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "vadimcn.vscode-lldb"
      ]
    }
  },
  "remoteUser": "coder"
}
